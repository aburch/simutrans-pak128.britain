# See "London Tramways" (John Reed, pub. Capital Transport, ISBN 1-85414-179-1) p. 44
obj=vehicle
name=lcc-m-class
engine_type=electric
speed=32
gear=80
# 2x 42hp motors 
power=63
tractive_effort=24
copyright=James/jamespetts
intro_year=1910
intro_month=8
retire_year=1931
retire_month=12
waytype=tram_track
freight=Passagiere
payload=62
overcrowded_capacity=4
length=4

weight=16.3
axles=4
cost=410000
runningcost=60
sound=-1

can_lead_from_rear=0
bidirectional=0
way_constraint_permissive[0]=1
way_constraint_prohibitive[1]=0
# Fully enclosed upper deck; no bogies
comfort=36
min_loading_time=10
max_loading_time=50

Constraint[Prev][1]=none
Constraint[Next][1]=none

# I cannot find information as to the differences between
# the LCC red livery of post 1926 and the LPTB livery.
# They were very similar, but must have been at least
# slightly different. Using the same graphics for now.

liverytype[1]=LCC-plum
liverytype[0]=LCC-red
liverytype[2]=LT

