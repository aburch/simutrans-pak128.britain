# See "London Tramways" (John Reed, pub. Capital Transport, ISBN 1-85414-179-1) p. 38
# and also http://www.lcctt.org.uk/106.htm (albeit that that page relates to the similar B class)
obj=vehicle
name=lcc-c-class
engine_type=electric
speed=32
gear=80
power=45
tractive_effort=27
copyright=James/jamespetts
intro_year=1903
intro_month=10
retire_year=1909
retire_month=2
waytype=tram_track
freight=Passagiere
payload=56
overcrowded_capacity=5
length=5

weight=11.5
axles=2
cost=365000
runningcost=56
sound=-1

can_lead_from_rear=0
bidirectional=0
way_constraint_permissive[0]=1
way_constraint_prohibitive[1]=0
comfort=34
min_loading_time=10
max_loading_time=52

Constraint[Prev][1]=none
Constraint[Next][1]=none

# I cannot find information as to the differences between
# the LCC red livery of post 1926 and the LPTB livery.
# They were very similar, but must have been at least
# slightly different. Using the same graphics for now.

liverytype[0]=LCC-plum
liverytype[1]=LCC-red
liverytype[2]=LT

