Obj=way
# This is really a waggonway
# reinforced with cast iron plates,
# rather than a true plateway
Name=plateway
waytype=track
cost=10000
maintenance=600
topspeed=12
# A higher axle load is allowed,
# but regular use of vehicles of this weight
# will wear this out very fast.
max_weight=4
intro_year=1767
intro_month=4
retire_year=1847
retire_month=9
# 25 passes of the Stephenson-Dodd Patent Engine
# (not counting tender and wagons - so fewer
# train passes)
wear_capacity=399375
#wear_capacity=13500000

