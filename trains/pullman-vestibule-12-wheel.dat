# Hamilton Ellis p. 102
weight=27.4
axles=4
cost=781500
runningcost=0
fixed_cost=770

can_lead_from_rear=0
bidirectional=1

# Inward opening doors
min_loading_time=40
max_loading_time=200

comfort[0]=0
comfort[1]=0
comfort[2]=0
comfort[3]=0
comfort[4]=150

# Intended to attach to any train.
Constraint[Next][0]=any

