# See http://gerald-massey.org.uk/Railway/carriages/Operational/Carriage3.png
# Little other information is available about these, but assume that they 
# are early third class open wagons from a time when second class was becoming
# routinely enclosed and third class passengers first started to be conveyed.
obj=vehicle
name=4wheel-open-third
speed=125
copyright=JamesPetts
intro_year=1837
intro_month=7
retire_year=1845
retire_month=4
waytype=track
freight=Passagiere
# The payload of the (unillustrated) Birmingham
# & Derby Junction thirds of 1839 was 40: see Lacy
# & Dow p. 6.  However, these are smaller carriages.
# Their capacity is assumed to be lower.
payload[0]=0
payload[1]=30
overcrowded_capacity=15
length=2
brake_force=0
rolling_resistance=19

weight=1.9
axles=2
cost=60000
runningcost=0
fixed_cost=125

comfort[0]=0
comfort[1]=16
min_loading_time=17
max_loading_time=47

can_lead_from_rear=0
bidirectional=1

Constraint[Prev][0]=any
Constraint[Next][0]=any

# TODO: Add liveries 
# (B&DJR, GJR, MCR, MR, LNWR, etc.)
